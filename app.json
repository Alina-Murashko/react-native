{
  "expo": {
    "name": "react-native-test",
    "slug": "react-native-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alinamurashko.reactnativetest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alinamurashko.reactnativetest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/SF-Pro-Display-Bold.otf",
            "./assets/fonts/SF-Pro-Display-Regular.otf",
            "./assets/fonts/SF-Pro-Display-Thin.otf",
            "./assets/fonts/Syncopate-Bold.otf",
            "./assets/fonts/Syncopate-Regular.otf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "db2cbdc4-bad8-442c-a5a0-b4734c9b3d1d"
      }
    }
  }
}
